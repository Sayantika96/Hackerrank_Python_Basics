   "source": [
    "def fibonacci():\n",
    "    x,y = 0,1\n",
    "    while True:\n",
    "        yield x\n",
    "        x,y = y, x+y\n",
    "        \n",
    "n = int(input(\"Enter a specified number of terms: \"))\n",
    "print(f\"Fibonacci sequence of first {n} numbers: \")\n",
    "\n",
    "fib = fibonacci()\n",
    "for _ in range(n):\n",
    "    print(next(fib),end=\" \")"
   ]
