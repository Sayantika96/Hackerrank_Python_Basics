    "def upper_case_decorator(func):\n",
    "    def wrapper():\n",
    "        result = func()\n",
    "        return result.upper()\n",
    "    return wrapper\n",
    "    \n",
    "@upper_case_decorator\n",
    "\n",
    "def get_input():\n",
    "    return input(\"Enter a string: \")\n",
    "\n",
    "get_input()"
